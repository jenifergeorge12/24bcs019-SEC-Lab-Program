<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        //set->can't accept duplicates, to uniquely identify the value's
        //create a set
        let email=new Set([]);
        email.add("js@gmail.com");
        email.add("java@gmail.com");
        email.add("python@gmail.com");
        console.log(email);
        email.delete("js@gmail.com");
        console.log(email);
        for (let mail of email){
            console.log(mail);
        } 
        email.forEach(x=>{
            console.log(x)
        })
        

        //example
        let adminpermission=new Set(["write","read","delete"])
        let userpermission=new Set(["read"]);
        function hasPermission(role,permission){
            return role.has(permission);
        }
        console.log(hasPermission(adminpermission,"delete"));
        console.log(hasPermission(userpermission,"delete"));
        

        //map
        let user1={name:"amaya"}
        let user2={name:"anisha"}
        let map=new WeakMap()
        map.set(user1,"welcome")
        map.set(user2,"hai")
        console.log(map)
        map.delete(user2)
        console.log(map)

        for(let detail of map.value){
            console.log(detail)
        }
        </script>
</body>
</html>
